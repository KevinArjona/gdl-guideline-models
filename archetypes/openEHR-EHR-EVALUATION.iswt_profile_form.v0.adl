definition
	EVALUATION[at0000] matches {    -- iswt_profile_form
		data matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0002] occurrences matches {0..1} matches {    -- Distance Walked
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::122]>
								list = <
									["1"] = <
										units = <"m">
										magnitude = <|>=0.0|>
									>
								>
							>
						}
					}
					ELEMENT[at0003] occurrences matches {0..1} matches {    -- Predicted HR Max
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::382]>
								list = <
									["1"] = <
										units = <"/min">
										magnitude = <|>=0.0|>
									>
								>
							>
						}
					}
					ELEMENT[at0004] occurrences matches {0..1} matches {    -- Predicted Distance
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::122]>
								list = <
									["1"] = <
										units = <"m">
										magnitude = <|>=0.0|>
									>
								>
							>
						}
					}
					ELEMENT[at0005] occurrences matches {0..1} matches {    -- % of Predicted Distance
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::507]>
								list = <
									["1"] = <
										units = <"%">
										magnitude = <|>=0.0|>
										precision = <|1|>
									>
								>
							>
						}
					}
					ELEMENT[at0006] occurrences matches {0..1} matches {    -- Predicted VO2 Peak
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::380]>
								list = <
									["1"] = <
										units = <"1">
									>
								>
							>
						}
					}
					ELEMENT[at0007] occurrences matches {0..1} matches {    -- Predicted Peak Exc Capacity
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::363]>
								list = <
									["1"] = <
										units = <"W">
									>
								>
							>
						}
					}
				}
			}
		}
	}

ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"iswt_profile_form">
					description = <"iswt_profile_form">
				>
				["at0001"] = <
					text = <"Item tree">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Distance Walked">
					description = <"This parameter is used to store the value of the distance walked by the individual on the ISWT performed in a standardized way.">
				>
				["at0003"] = <
					text = <"Predicted HR Max">
					description = <"This parameter stores the predicted maximum heart rate (beats/min) for an individual based on their age (in years).">
				>
				["at0004"] = <
					text = <"Predicted Distance">
					description = <"This parameter stores the predicted distance that an individual is expected to walk on the ISWT based on their age, gender and body mass index.">
				>
				["at0005"] = <
					text = <"% of Predicted Distance">
					description = <"This parameter stores the value of the following:
% of Predicted Distance = Distance Walked / Predicted Distance * 100">
				>
				["at0006"] = <
					text = <"Predicted VO2 Peak">
					description = <"VO2 peak=4.19+0.025 (Distance Walked)
where VO2peak is in millilitres of oxygen per kilogram of the body mass per minute and distance is in meters.">
				>
				["at0007"] = <
					text = <"Predicted Peak Exc Capacity">
					description = <"W peak = (4.19 + 0.025 × distance walked) × weight.
where W peak is the peak exercise capacity in watts,
distance walked is in meters,
weight is in kg">
				>
			>
		>
	>
