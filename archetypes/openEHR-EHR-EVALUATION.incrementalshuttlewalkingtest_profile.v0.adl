definition
	EVALUATION[at0000] matches {    -- incrementalshuttlewalkingtest_profile
		data matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0002] occurrences matches {0..1} matches {    -- Distance_walked
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::122]>
								list = <
									["1"] = <
										units = <"m">
										magnitude = <|>0.0|>
									>
								>
							>
						}
					}
					ELEMENT[at0004] occurrences matches {0..1} matches {    -- 85%_Pred_HR_Max
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::382]>
								list = <
									["1"] = <
										units = <"/min">
										magnitude = <|>0.0|>
									>
								>
							>
						}
					}
					ELEMENT[at0005] occurrences matches {0..1} matches {    -- Pred_Distance
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::122]>
								list = <
									["1"] = <
										units = <"m">
										magnitude = <|>0.0|>
									>
								>
							>
						}
					}
					ELEMENT[at0006] occurrences matches {0..1} matches {    -- %of_Pred_Distance
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::507]>
								list = <
									["1"] = <
										units = <"%">
										magnitude = <|>0.0|>
									>
								>
							>
						}
					}
					ELEMENT[at0007] occurrences matches {0..1} matches {    -- VO2_Peak
						value matches {
							DV_QUANTITY matches {*}
						}
					}
					ELEMENT[at0008] occurrences matches {0..1} matches {    -- Peak_Exc_Capacity
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::363]>
								list = <
									["1"] = <
										units = <"W">
										magnitude = <|>0.0|>
									>
								>
							>
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0003] matches {*}    -- Item tree
		}
	}

ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"incrementalshuttlewalkingtest_profile">
					description = <"IncrementaShuttleWalkingTest_Profile gives a report of all outcome measures related to exercise capacity that are derived from the distance walked by an individual on the Incremental Shuttle Walking Test (ISWT)">
				>
				["at0001"] = <
					text = <"Item tree">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Distance_walked">
					description = <"This parameter is used to store the value of the distance walked by the individual on the ISWT performed in a standardized way.">
				>
				["at0003"] = <
					text = <"Item tree">
					description = <"@ internal @">
				>
				["at0004"] = <
					text = <"85%_Pred_HR_Max">
					description = <"This parameter stores the predicted maximum heart rate (beats/min) for an individual based on their age (in years).">
				>
				["at0005"] = <
					text = <"Pred_Distance">
					description = <"This parameter stores the predicted distance that an individual is expected to walk on the ISWT based on their age, gender and body mass index.">
				>
				["at0006"] = <
					text = <"%of_Pred_Distance">
					description = <"This parameter stores the value of the following:
%of_Pred_Distance = Distance_walked/ Pred_Distance * 100">
				>
				["at0007"] = <
					text = <"VO2_Peak">
					description = <"VO2peak=4.19+0.025 (distance walked)
where VO2peak is in ml•min-1•kg-1 and distance is in meters.
">
				>
				["at0008"] = <
					text = <"Peak_Exc_Capacity">
					description = <"W peak = (4.19 + 0.025 × distance walked) × weight.
where W peak is the peak exercise capacity in watts,
distance walked is in meters,
weight is in kg">
				>
			>
		>
	>
