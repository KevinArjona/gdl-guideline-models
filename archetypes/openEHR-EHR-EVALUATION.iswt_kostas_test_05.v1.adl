definition
	EVALUATION[at0000] matches {    -- iswt_kostas_test_05
		data matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0002] occurrences matches {0..1} matches {    -- Distance Walked
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::122]>
								list = <
									["1"] = <
										units = <"m">
										magnitude = <|>0.0|>
									>
								>
							>
						}
					}
					ELEMENT[at0004] occurrences matches {0..1} matches {    -- Predicted HR Max
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::382]>
								list = <
									["1"] = <
										units = <"/min">
										magnitude = <|>0.0|>
									>
								>
							>
						}
					}
					ELEMENT[at0005] occurrences matches {0..1} matches {    -- Predicted Distance
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::122]>
								list = <
									["1"] = <
										units = <"m">
										magnitude = <|>0.0|>
									>
								>
							>
						}
					}
					ELEMENT[at0006] occurrences matches {0..1} matches {    -- % of Predicted Distance
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::507]>
								list = <
									["1"] = <
										units = <"%">
										magnitude = <|>0.0|>
									>
								>
							>
						}
					}
					ELEMENT[at0007] occurrences matches {0..1} matches {    -- VO2 Peak
						value matches {
							DV_QUANTITY matches {*}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0003] matches {*}    -- Item tree
		}
	}

